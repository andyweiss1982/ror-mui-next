<%= react_component(
    'ChangePassword',
    props: {
              minimum_password_length:  @minimum_password_length,
              reset_password_token:     params[:reset_password_token],
              alert: {
                password:               flash[:password]&.join(', '),
                password_confirmation:  flash[:password_confirmation]&.join(', '),
              }
            }
) %>
